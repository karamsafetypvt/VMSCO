
<form enctype="multipart/form-data" id="FormQuickAddVisitor" method="post">
    <table class="table" border="0" id="tblQuestionnaire">
        <tbody>
            <tr>
                <td style="vertical-align:top;">
                    1.
                </td>
                <td style="padding-right: 10px">
                    <b>What is your current body temperature?</b>
                </td>
                <td>
                    <input id="txtLabelQuestionQ" name="Q1" required class="clsoptiontext form-control" style="border-radius: 4px; border-style: solid; border-color: rgb(204, 204, 204);border-width: 1px;" type="text" onkeypress="return isNumberAndDotKey(event)">
                    <input type="hidden" id="GP_ID_Question" />
                </td>
            </tr>
            <tr>
                <td style="vertical-align:top;">
                    2.
                    <input value="2" class="hdclsSeqno" type="hidden">
                </td>
                <td style="padding-right: 10px">
                    <b>Have you traveled anywhere in country or outside the country in past 20 days?</b>
                </td>
                <td>
                    <label>Yes</label>
                    <input required style="margin-left:
                            10px;" name="Q2" class="clsoptiontext" id="clscheckbox-2Q2" type="radio" value="Yes">
                    <label>No</label>
                    <input required style="margin-left:
                            10px;" name="Q2" class="clsoptiontext" id="clscheckbox-2Q2" checked type="radio" value="No">
                </td>
            </tr>
            <tr>
                <td style="vertical-align:top;">
                    3.
                    <input value="3" class="hdclsSeqno" type="hidden">
                </td>
                <td style="padding-right: 10px">
                    <b>Have you installed &amp; configured Arogya Setu application in your phone?</b>
                </td>
                <td>
                    <label>Yes</label>
                    <input style="margin-left:
                            10px;" name="Q3" class="clsoptiontext" id="clscheckbox-3Q3" type="radio" required value="Yes" checked>
                    <label>No</label>
                    <input style="margin-left:
                            10px;" name="Q3" class="clsoptiontext" id="clscheckbox-3Q3" type="radio" required value="No">
                </td>
            </tr>
            <tr>
                <td style="vertical-align:top;">
                    4.
                    <input value="4" class="hdclsSeqno" type="hidden">
                </td>
                <td style="padding-right: 10px">
                    <b>Are you suffering from High Temperature/Cough/Cold?</b>
                </td>
                <td>
                    <label>Yes</label>
                    <input style="margin-left:
                            10px;" name="Q4" class="clsoptiontext" id="clscheckbox-4Q4" type="radio" required value="Yes">
                    <label>No</label>
                    <input style="margin-left:
                            10px;" name="Q4" class="clsoptiontext" id="clscheckbox-4Q4" type="radio" required value="No" checked>
                </td>
            </tr>
        </tbody>
    </table>
    <div class="ui-dialog-buttonpane ui-widget-content ui-helper-clearfix">
        <div class="ui-dialog-buttonset">
            <input class="btn btn-primary" type="button" id="btnQuestion" value="Save" />
        </div>
    </div>
</form>