<form method="post">
    <div class="form-horizontal col-md-12">
        <div class="main">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="widget-content">
                            <div class="row">
                                <div class="col-md-12">
                                    <fieldset>
                                        <div class="form-group">
                                            <label for="name" class="col-md-12">Company Name <i class="fa fa-asterisk required"></i></label>
                                            <div class="col-md-10">
                                                <input class="form-control" data-val="true" data-val-required="The VisitorCompanyName field is required." onkeypress="return isAlphabt(event)" id="fVisitorCompanyName" maxlength="100" ng-model="Company.CName" value="" type="text">
                                                <span id="ValidationForVisitorCompanyNamed" style="color: Red; display: none" class="field-validation-error">
                                                </span>
                                            </div>
                                            <div class="col-md-2">
                                                @*<i class="fa fa-asterisk required" style="margin-left: -25px;"></i>*@
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="name" class="col-md-10">Company Address</label>
                                            <div class="col-md-10">
                                                <textarea class="form-control" cols="20" id="fAddress" maxlength="255" rows="2" ng-model="Company.Addres"></textarea>
                                                <span id="ValidationForAddress1" class="field-validation-error" style="display: none">
                                                </span>
                                            </div>
                                            <div class="col-md-2">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="name" class="col-md-10">Phone Number</label>
                                            <div class="col-md-10">
                                                <input class="form-control" onkeypress="return CheckPhone(event)" id="fPhone" maxlength="10" ng-model="Company.Phone" value="" type="text"><br>

                                                <span id="ValidationForPhone1" class="field-validation-error" style="display: none">
                                                </span>
                                            </div>
                                            <div class="col-md-2">
                                            </div>
                                        </div>

                                    </fieldset>
                                </div>

                            </div>
                            <div class="row">
                                <input id="hdnCountUDF" value="0" type="hidden">
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="ErrorDiv" id="MessageForm" style="display: none">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="ui-dialog-buttonpane ui-widget-content ui-helper-clearfix">
        <div class="ui-dialog-buttonset">
            <input type="button" value="Save" id="btnSave123" ng-click="savedata()" class="btn btn-primary" />
            @*<button class="btn btn-primary dialogClose" type="button">Close</button>*@
        </div>
    </div>
</form>

<script src="~/Login/js/cleave.js"></script>
<script src="~/Login/js/cleave-phone.in.js"></script>
<script>
    $(document).ready(function () {
        new Cleave('#fPhone', {
            phone: true,
            //phoneRegionCode: 'IN'
        })
    })






</script>
<script src="~/Layout/Scripts/jtable.2.3.0/jquery.jtable.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script>


    $("#btnSave123").click(function () {

        var fvVisitorName = $("#fVisitorCompanyName").val().trim();

        if (fvVisitorName == "") {

            alert("please enter Company");
            return false;

        }
    });


</script>



