                    </select>
                    @*readonly*@



                    <label>Remark</label>

                    <textarea class="form-control" maxlength="200" required name="CouriarRemark" value="NA">Dispatch</textarea>
                </div>
                <div class="modal-footer">
                  
                    <button type="submit" id="btnSave" class="btn btn-danger">@*Close*@Dispatch</button>
                </div>
            </form>
        </div>
    </div>
</div>
