<script>

    $(document).ready(function () {
        //$("#AppointmentDateString").datepicker({
        //    'autoclose': true,
        //    /* 'startDate': '-0m'*/
        //});


        //$(".validate").datepicker({
        //    'autoclose': true,
        //    'endDate': '-0m',
        //    'format': 'dd/M/yyyy'
        //});
    });

    //*********Current Time Setting 
    var d = new Date();
    var h = d.getHours();
    var m = d.getMinutes();
    // Check whether AM or PM
    var newformat = h >= 12 ? 'PM' : 'AM';
    if (h > 12) {

        h = h - 12;
    }

    let text = h;
    //alert(text);
    $(".clh option").filter(function () {

        return $(this).text() == h;
    }).attr('selected', true);

    $(".clm option").filter(function () {

        return $(this).text() == m;
    }).attr('selected', true);


    $(".clam option").filter(function () {

        return $(this).text() == newformat;
    }).attr('selected', true)


    //********End Time Setting







    $("#btnSave").click(function () {

        //txtremark txtvendor txtdocket txtRecievetime txtdoc txtcity txtcompany txtdept  AppointmentDateString V_DateFrom
        var Hour = $('#hh option:selected').text();
        var Min = $('#mm option:selected').text();
        var ampm = $('#ampm option:selected').text();
        
        var ddlPacketType = $('#ddlPacketType option:selected').text();
       // var ddlfromcity = $('#ddlfromcity option:selected').text();
        var stime = Hour + ":" + Min + ampm;

     
        if ($("#AppointmentDateString").val() == "") {
            alert("Please Enter Date");
            return false;
        }
        else if (ddlPacketType == "--Please Select--") {
            alert("Please Select Packet Type");
            return false;
        }
        
        else if ($("#txtEmpId").val() == "") {
            alert("Please Enter EmpId");
            return false;
        }
        else if ($("#txtCompany").val() == "") {
            alert("Please Enter Company");
            return false;
        }

        else if ($("#txtPerson").val() == "") {
            alert("Please Enter Person");
            return false;
        }
       

     
      
        else if ($("#txtContactNo").val() == "") {
            alert("Please Enter Contact No.");
            return false;
        }

        else if ($("#txtContactNo").val().length<2) {
            alert("Please Enter correct Contact No.");
            return false;


        }



        else if ($("#txtAddress").val() == "") {
            alert("Please Enter Address");
            return false;
        }
        else if ($("#txtFromcity").val() == "") {
            alert("Please Enter From  City");
            return false;
        }

        else if ($("#txtcity").val() == "") {
            alert("Please Enter To City");
            return false;
        }

        else if ($("#hh").val() == "-1") {
    /*    else if ($("#hh").val() == "0") {*/
            alert("Please Select Hour");
            return false;
        }
        else if ($("#mm").val() == "0") {
            alert("Please Select Minute");
            return false;
        }


      
        //else if ($("#txtRecievetime").val() == "") {
        //    alert("Please Enter Courier Receiving Time");
        //    return false;
        //}
        else if ($("#txtdocket").val() == "") {
            alert("Please Enter Docket No.");
            return false;
        }
        else if ($("#txtvendor").val() == "") {
            alert("Please Enter  Vendor Name");
            return false;
        }
        else if ($("#txtdoc").val() == "") {
            alert("Please Enter Document");
            return false;
        }


      

        $("#txtRecievetime").val(stime);
     
    });


</script>

