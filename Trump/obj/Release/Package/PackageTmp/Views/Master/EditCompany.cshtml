@model IEnumerable<Trump.Models.tblCompany>
@{
    ViewBag.Title = "EditCompany";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content" style="font-size:12px;min-height:100%">
    <div class="content_resize">
        <div class="mainbar">
            <div class="article">
                <div class="main">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="widget stacked">
                                    <div class="widget-header">
                                        <i class="icon-ok"></i>
                                        <h3>
                                            Edit Company
                                        </h3>
                                    </div>
                                    <form action="../Master/EditCompany" method="post" enctype="multipart/form-data">
                                        <input type="hidden" value="@Model.Select(z => z.C_ID).FirstOrDefault()" name="hidd" />
                                        <div class="widget-content">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="name" class="col-md-12">
                                                            Compane Name   <i class="fa fa-asterisk required" ></i>
                                                        </label>
                                                        <div class="col-md-10">
                                                            <input type="text" name="E_Cname" id="E_Cname" class="form-control" value="@Model.Select(z => z.CName).FirstOrDefault()"  />
                                                        </div>
                                                        <div class="col-md-2">
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label for="name" class="col-md-8">
                                                            Address
                                                        </label>
                                                        <div class="col-md-10">
                                                            <input class="form-control" data-val="true" name="E_Caddress" value="@Model.Select(z=>z.Addres).FirstOrDefault()" type="text">
                                                        </div>
                                                        <div class="col-md-2">
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="name" class="col-md-8">Phone Number</label>
                                                        <div class="col-md-10">
                                                            <input type="text" name="E_Cnum" onkeypress="return CheckPhone(event)" maxlength="10" value="@Model.Select(z=>z.Phone).FirstOrDefault()" class="form-control" />
                                                        </div>
                                                        <div class="col-md-2">
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="name" class="col-md-12">Status</label>
                                                        <div class="col-md-10">
                                                            <select tabindex="-1" class="form-control select2_group ddlIDType" data-val="true" name="E_CStatus">
                                                                <option selected>@Model.Select(z => z.C_Status).FirstOrDefault()</option>
                                                                <option>Active</option>
                                                                <option>Inactive</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-2">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <hr>

                                            <div class="row">
                                                <div class="container">
                                                    <input class="btn btn-primary btnSave" id="btnSave" value="Update" type="submit">
                                                    <input class="btn btn-primary" value="Back" type="button" onclick="goBack()">
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="clr">
        </div>
    </div>
</div>
<script src="~/Layout/Scripts/jtable.2.3.0/jquery.jtable.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script>



    function goBack() {
        // window.history.back();

        window.location.href = "../Master/VisitorCompany";
    }






</script>

<script>

    $("#btnSave").click(function () {
       
        var fvVisitorName = $("#E_Cname").val().trim();

        if (fvVisitorName == "") {

            alert("please enter Company");
            return false;

        }
    });


</script>