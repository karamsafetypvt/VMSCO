        <a href="#" id="aboutbtn">
            <i class="fa fa-cog fa-qrcode" style="font-size: large;
            color: White; border-top-right-radius: 0; border-top-left-radius: 0;">&nbsp;</i>
        </a>
        <input id="txtEntryCode" autofocus value="" class="txtbox" style=" width: 225px;
        color: Black;
        background-color: White;
        display: inline-block;
        font-family: Verdana;" focus="" placeholder="Scan here" title="Scan here" type="text">
