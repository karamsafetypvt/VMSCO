</div>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script>
    $(document).ready(function () {
        $("#dropdownArrow").click(function () {
            $("#rgpContainer").slideToggle("slow");
        })
    })
</script>
<script>
