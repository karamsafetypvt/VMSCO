        var txtDept = $('#secDept').val();

        var stime = Hour + ":" + Min + ampm;


        if ($("#AppointmentDateString").val() == "") {
            alert("Please Enter Date");
            return false;
        }

        else if ($("#txtdocket").val() == "") {
            alert("Please Enter Docket No.");
            return false;
        }
        else if (ddlCourierCompany == "--Please Select--") {

            alert("Please Select Delivery Provider Name");
            return false;
        }
        else if ($("#txtdoc").val() == "") {
            alert("Please Enter Document");
            return false;
        }
        else if ($("#txtSendername").val() == "") {
            alert("Please Enter Sender Name");
            return false;

        }

        else if (ddlCity == "--Please Select--") {
            alert("Please Select City");
            return false;
        }
        /*txtcompany*/
        else if ($("#Req_sec").val() == "") {
            alert("Please Enter Recipient’s Name ");
            return false;
        }
        else if (txtRegionNew == "") {
            alert("Please Enter Region");
