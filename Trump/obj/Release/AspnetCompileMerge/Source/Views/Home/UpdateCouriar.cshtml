    //*********Current Time Setting
    //var d = new Date();
    var h;
    var m ;
    h = $("#txthour").val();
    m = $("#txtmin").val();
   
    // Check whether AM or PM
    var newformat = h >= 12 ? 'PM' : 'AM';
    if (h > 12) {

        h = h - 12;
    }

    if (m.length == 1)
    {
        m = "0" + m;
    }
    newformat = $("#txtampm").val();
      
  
    let text = h;
    //alert(text);
    $(".clh option").filter(function () {

        return $(this).text() == h;
    }).attr('selected', true);

    $(".clm option").filter(function () {

        return $(this).text() == m;
    }).attr('selected', true);


    $(".clam option").filter(function () {

        return $(this).text() == newformat;
    }).attr('selected', true)


    //********End Time Setting



   

    $("#btnSave").click(function () {


