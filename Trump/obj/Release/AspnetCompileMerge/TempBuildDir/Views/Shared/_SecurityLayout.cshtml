                $(".dialog-formVisitorAdd").dialog("open");
            })
            $("#divCam").change(function () {
                $('#divVis').empty();
                var d = $("#divCam").val()
                $.ajax({
                    type: "GET",
                    url: '@Url.Action("GetVisitorData", "Home")',
                    data: { ID: d },
                    dataType: "JSON",
                    success: function (data) {
                        $(data).each(function () {
                        $('#divVis').append(
                            $('<option/>', {
                                value: this.V_ID
                            }).html(this.V_Name)
                        );
                });
                    }
                })
            })
        })
    </script>

